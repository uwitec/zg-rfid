<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdRestypeAttrset">

    <typeAlias alias="tmdRestypeAttrset" type="com.boco.frame.meta.base.model.TmdRestypeAttrset"/>

    <resultMap id="tmdRestypeAttrsetResult" class="tmdRestypeAttrset">
        <result property="cuid" column="CUID"/>
        <result property="relatedTypeCuid" column="RELATED_TYPE_CUID"/>
        <result property="relatedAttrsetCuid" column="RELATED_ATTRSET_CUID"/>
        <result property="description" column="DESCRIPTION"/>
        <result property="isBasic" column="IS_BASIC"/>
        <result property="isSystem" column="IS_SYSTEM"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.RELATED_TYPE_CUID,
        	t.RELATED_ATTRSET_CUID,
        	t.DESCRIPTION,
        	t.IS_BASIC,
        	t.IS_SYSTEM
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedTypeCuid">
	       			t.RELATED_TYPE_CUID like #relatedTypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedAttrsetCuid">
	       			t.RELATED_ATTRSET_CUID like #relatedAttrsetCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="description">
	       			t.DESCRIPTION like #description#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isBasic">
	       			t.IS_BASIC = #isBasic#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isSystem">
	       			t.IS_SYSTEM = #isSystem#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdRestypeAttrset">
    <![CDATA[
        INSERT INTO
        T_MD_RESTYPE_ATTRSET (
        	CUID ,
        	RELATED_TYPE_CUID ,
        	RELATED_ATTRSET_CUID ,
        	DESCRIPTION ,
        	IS_BASIC ,
        	IS_SYSTEM 
        ) VALUES (
        	#cuid# ,
        	#relatedTypeCuid# ,
        	#relatedAttrsetCuid# ,
        	#description# ,
        	#isBasic# ,
        	#isSystem# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdRestypeAttrset">
    <![CDATA[
        UPDATE T_MD_RESTYPE_ATTRSET SET
	        RELATED_TYPE_CUID = #relatedTypeCuid# ,
	        RELATED_ATTRSET_CUID = #relatedAttrsetCuid# ,
	        DESCRIPTION = #description# ,
	        IS_BASIC = #isBasic# ,
	        IS_SYSTEM = #isSystem# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_RESTYPE_ATTRSET where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_RESTYPE_ATTRSET t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdRestypeAttrsetResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RESTYPE_ATTRSET t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_RESTYPE_ATTRSET t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdRestypeAttrsetResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RESTYPE_ATTRSET t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdRestypeAttrsetResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RESTYPE_ATTRSET t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdRestypeAttrsetResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_RESTYPE_ATTRSET t
	    ]]>
    </select>
</sqlMap>
