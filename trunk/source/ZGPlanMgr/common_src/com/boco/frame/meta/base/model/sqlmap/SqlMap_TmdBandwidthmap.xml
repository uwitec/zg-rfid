<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdBandwidthmap">

    <typeAlias alias="tmdBandwidthmap" type="com.boco.frame.meta.base.model.TmdBandwidthmap"/>

    <resultMap id="tmdBandwidthmapResult" class="tmdBandwidthmap">
        <result property="cuid" column="CUID"/>
        <result property="relatedBandwidthCuidParent" column="RELATED_BANDWIDTH_CUID_PARENT"/>
        <result property="relatedBandwidthCuidChild" column="RELATED_BANDWIDTH_CUID_CHILD"/>
        <result property="compositenum" column="COMPOSITENUM"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.RELATED_BANDWIDTH_CUID_PARENT,
        	t.RELATED_BANDWIDTH_CUID_CHILD,
        	t.COMPOSITENUM
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedBandwidthCuidParent">
	       			t.RELATED_BANDWIDTH_CUID_PARENT like #relatedBandwidthCuidParent#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedBandwidthCuidChild">
	       			t.RELATED_BANDWIDTH_CUID_CHILD like #relatedBandwidthCuidChild#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="compositenum">
	       			t.COMPOSITENUM = #compositenum#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdBandwidthmap">
    <![CDATA[
        INSERT INTO
        T_MD_BANDWIDTHMAP (
        	CUID ,
        	RELATED_BANDWIDTH_CUID_PARENT ,
        	RELATED_BANDWIDTH_CUID_CHILD ,
        	COMPOSITENUM 
        ) VALUES (
        	#cuid# ,
        	#relatedBandwidthCuidParent# ,
        	#relatedBandwidthCuidChild# ,
        	#compositenum# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdBandwidthmap">
    <![CDATA[
        UPDATE T_MD_BANDWIDTHMAP SET
	        RELATED_BANDWIDTH_CUID_PARENT = #relatedBandwidthCuidParent# ,
	        RELATED_BANDWIDTH_CUID_CHILD = #relatedBandwidthCuidChild# ,
	        COMPOSITENUM = #compositenum# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_BANDWIDTHMAP where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_BANDWIDTHMAP t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdBandwidthmapResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_BANDWIDTHMAP t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_BANDWIDTHMAP t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdBandwidthmapResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_BANDWIDTHMAP t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdBandwidthmapResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_BANDWIDTHMAP t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdBandwidthmapResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_BANDWIDTHMAP t
	    ]]>
    </select>
</sqlMap>
