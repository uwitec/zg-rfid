<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdCardtype">

    <typeAlias alias="tmdCardtype" type="com.boco.frame.meta.base.model.TmdCardtype"/>

    <resultMap id="tmdCardtypeResult" class="tmdCardtype">
        <result property="cuid" column="CUID"/>
        <result property="bmClassid" column="BM_CLASSID"/>
        <result property="labelCn" column="LABEL_CN"/>
        <result property="length" column="LENGTH"/>
        <result property="width" column="WIDTH"/>
        <result property="portstartpos" column="PORTSTARTPOS"/>
        <result property="slotstartpos" column="SLOTSTARTPOS"/>
        <result property="dynport" column="DYNPORT"/>
        <result property="dynslot" column="DYNSLOT"/>
        <result property="description" column="DESCRIPTION"/>
        <result property="isSubcard" column="IS_SUBCARD"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.BM_CLASSID,
        	t.LABEL_CN,
        	t.LENGTH,
        	t.WIDTH,
        	t.PORTSTARTPOS,
        	t.SLOTSTARTPOS,
        	t.DYNPORT,
        	t.DYNSLOT,
        	t.DESCRIPTION,
        	t.IS_SUBCARD
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="bmClassid">
	       			t.BM_CLASSID like #bmClassid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="labelCn">
	       			t.LABEL_CN like #labelCn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="length">
	       			t.LENGTH = #length#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="width">
	       			t.WIDTH = #width#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="portstartpos">
	       			t.PORTSTARTPOS = #portstartpos#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="slotstartpos">
	       			t.SLOTSTARTPOS = #slotstartpos#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="dynport">
	       			t.DYNPORT = #dynport#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="dynslot">
	       			t.DYNSLOT = #dynslot#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="description">
	       			t.DESCRIPTION like #description#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isSubcard">
	       			t.IS_SUBCARD = #isSubcard#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdCardtype">
    <![CDATA[
        INSERT INTO
        T_MD_CARDTYPE (
        	CUID ,
        	BM_CLASSID ,
        	LABEL_CN ,
        	LENGTH ,
        	WIDTH ,
        	PORTSTARTPOS ,
        	SLOTSTARTPOS ,
        	DYNPORT ,
        	DYNSLOT ,
        	DESCRIPTION ,
        	IS_SUBCARD 
        ) VALUES (
        	#cuid# ,
        	#bmClassid# ,
        	#labelCn# ,
        	#length# ,
        	#width# ,
        	#portstartpos# ,
        	#slotstartpos# ,
        	#dynport# ,
        	#dynslot# ,
        	#description# ,
        	#isSubcard# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdCardtype">
    <![CDATA[
        UPDATE T_MD_CARDTYPE SET
	        BM_CLASSID = #bmClassid# ,
	        LABEL_CN = #labelCn# ,
	        LENGTH = #length# ,
	        WIDTH = #width# ,
	        PORTSTARTPOS = #portstartpos# ,
	        SLOTSTARTPOS = #slotstartpos# ,
	        DYNPORT = #dynport# ,
	        DYNSLOT = #dynslot# ,
	        DESCRIPTION = #description# ,
	        IS_SUBCARD = #isSubcard# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_CARDTYPE where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_CARDTYPE t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdCardtypeResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_CARDTYPE t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_CARDTYPE t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdCardtypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_CARDTYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdCardtypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_CARDTYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdCardtypeResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_CARDTYPE t
	    ]]>
    </select>
</sqlMap>
