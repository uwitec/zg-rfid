<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdPortdef">

    <typeAlias alias="tmdPortdef" type="com.boco.frame.meta.base.model.TmdPortdef"/>

    <resultMap id="tmdPortdefResult" class="tmdPortdef">
        <result property="cuid" column="CUID"/>
        <result property="labelCn" column="LABEL_CN"/>
        <result property="relatedDevicetypeCuid" column="RELATED_DEVICETYPE_CUID"/>
        <result property="relatedCardtypeCuid" column="RELATED_CARDTYPE_CUID"/>
        <result property="relatedPorttypeCuid" column="RELATED_PORTTYPE_CUID"/>
        <result property="sequence" column="SEQUENCE"/>
        <result property="rowindevice" column="ROWINDEVICE"/>
        <result property="colindevice" column="COLINDEVICE"/>
        <result property="orientation" column="ORIENTATION"/>
        <result property="relatedBandwidthCuid" column="RELATED_BANDWIDTH_CUID"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.LABEL_CN,
        	t.RELATED_DEVICETYPE_CUID,
        	t.RELATED_CARDTYPE_CUID,
        	t.RELATED_PORTTYPE_CUID,
        	t.SEQUENCE,
        	t.ROWINDEVICE,
        	t.COLINDEVICE,
        	t.ORIENTATION,
        	t.RELATED_BANDWIDTH_CUID
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="labelCn">
	       			t.LABEL_CN like #labelCn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedDevicetypeCuid">
	       			t.RELATED_DEVICETYPE_CUID like #relatedDevicetypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedCardtypeCuid">
	       			t.RELATED_CARDTYPE_CUID like #relatedCardtypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedPorttypeCuid">
	       			t.RELATED_PORTTYPE_CUID like #relatedPorttypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="sequence">
	       			t.SEQUENCE = #sequence#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="rowindevice">
	       			t.ROWINDEVICE = #rowindevice#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="colindevice">
	       			t.COLINDEVICE = #colindevice#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="orientation">
	       			t.ORIENTATION = #orientation#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedBandwidthCuid">
	       			t.RELATED_BANDWIDTH_CUID like #relatedBandwidthCuid#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdPortdef">
    <![CDATA[
        INSERT INTO
        T_MD_PORTDEF (
        	CUID ,
        	LABEL_CN ,
        	RELATED_DEVICETYPE_CUID ,
        	RELATED_CARDTYPE_CUID ,
        	RELATED_PORTTYPE_CUID ,
        	SEQUENCE ,
        	ROWINDEVICE ,
        	COLINDEVICE ,
        	ORIENTATION ,
        	RELATED_BANDWIDTH_CUID 
        ) VALUES (
        	#cuid# ,
        	#labelCn# ,
        	#relatedDevicetypeCuid# ,
        	#relatedCardtypeCuid# ,
        	#relatedPorttypeCuid# ,
        	#sequence# ,
        	#rowindevice# ,
        	#colindevice# ,
        	#orientation# ,
        	#relatedBandwidthCuid# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdPortdef">
    <![CDATA[
        UPDATE T_MD_PORTDEF SET
	        LABEL_CN = #labelCn# ,
	        RELATED_DEVICETYPE_CUID = #relatedDevicetypeCuid# ,
	        RELATED_CARDTYPE_CUID = #relatedCardtypeCuid# ,
	        RELATED_PORTTYPE_CUID = #relatedPorttypeCuid# ,
	        SEQUENCE = #sequence# ,
	        ROWINDEVICE = #rowindevice# ,
	        COLINDEVICE = #colindevice# ,
	        ORIENTATION = #orientation# ,
	        RELATED_BANDWIDTH_CUID = #relatedBandwidthCuid# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_PORTDEF where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_PORTDEF t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdPortdefResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_PORTDEF t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_PORTDEF t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdPortdefResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_PORTDEF t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdPortdefResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_PORTDEF t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdPortdefResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_PORTDEF t
	    ]]>
    </select>
</sqlMap>
