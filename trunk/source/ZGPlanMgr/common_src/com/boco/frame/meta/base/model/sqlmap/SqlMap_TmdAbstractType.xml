<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdAbstractType">

    <typeAlias alias="tmdAbstractType" type="com.boco.frame.meta.base.model.TmdAbstractType"/>

    <resultMap id="tmdAbstractTypeResult" class="tmdAbstractType">
        <result property="cuid" column="CUID"/>
        <result property="parentTypeCuid" column="PARENT_TYPE_CUID"/>
        <result property="bmClassid" column="BM_CLASSID"/>
        <result property="labelCn" column="LABEL_CN"/>
        <result property="relatedResclassCuid" column="RELATED_RESCLASS_CUID"/>
        <result property="labelAttrId" column="LABEL_ATTR_ID"/>
        <result property="labelJoinChar" column="LABEL_JOIN_CHAR"/>
        <result property="objectSelectXtype" column="OBJECT_SELECT_XTYPE"/>
        <result property="isShowAddBtn" column="IS_SHOW_ADD_BTN"/>
        <result property="isShowEditBtn" column="IS_SHOW_EDIT_BTN"/>
        <result property="isShowDeleteBtn" column="IS_SHOW_DELETE_BTN"/>
        <result property="isShowExportBtn" column="IS_SHOW_EXPORT_BTN"/>
        <result property="isShowImportBtn" column="IS_SHOW_IMPORT_BTN"/>
        <result property="isVirtual" column="IS_VIRTUAL"/>
        <result property="pkCol" column="PK_COL"/>
        <result property="typeTableName" column="TYPE_TABLE_NAME"/>
        <result property="flag" column="FLAG"/>
        <result property="usetype" column="USETYPE"/>
        <result property="isShowType" column="IS_SHOW_TYPE"/>
        <result property="treeSort" column="TREE_SORT"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.PARENT_TYPE_CUID,
        	t.BM_CLASSID,
        	t.LABEL_CN,
        	t.RELATED_RESCLASS_CUID,
        	t.LABEL_ATTR_ID,
        	t.LABEL_JOIN_CHAR,
        	t.OBJECT_SELECT_XTYPE,
        	t.IS_SHOW_ADD_BTN,
        	t.IS_SHOW_EDIT_BTN,
        	t.IS_SHOW_DELETE_BTN,
        	t.IS_SHOW_EXPORT_BTN,
        	t.IS_SHOW_IMPORT_BTN,
        	t.IS_VIRTUAL,
        	t.PK_COL,
        	t.TYPE_TABLE_NAME,
        	t.FLAG,
        	t.USETYPE,
        	t.IS_SHOW_TYPE,
        	t.TREE_SORT
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="parentTypeCuid">
	       			t.PARENT_TYPE_CUID like #parentTypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="bmClassid">
	       			t.BM_CLASSID like #bmClassid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="labelCn">
	       			t.LABEL_CN like #labelCn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedResclassCuid">
	       			t.RELATED_RESCLASS_CUID like #relatedResclassCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="labelAttrId">
	       			t.LABEL_ATTR_ID like #labelAttrId#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="labelJoinChar">
	       			t.LABEL_JOIN_CHAR like #labelJoinChar#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="objectSelectXtype">
	       			t.OBJECT_SELECT_XTYPE like #objectSelectXtype#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isShowAddBtn">
	       			t.IS_SHOW_ADD_BTN = #isShowAddBtn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isShowEditBtn">
	       			t.IS_SHOW_EDIT_BTN = #isShowEditBtn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isShowDeleteBtn">
	       			t.IS_SHOW_DELETE_BTN = #isShowDeleteBtn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isShowExportBtn">
	       			t.IS_SHOW_EXPORT_BTN = #isShowExportBtn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isShowImportBtn">
	       			t.IS_SHOW_IMPORT_BTN = #isShowImportBtn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isVirtual">
	       			t.IS_VIRTUAL = #isVirtual#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="pkCol">
	       			t.PK_COL like #pkCol#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="typeTableName">
	       			t.TYPE_TABLE_NAME like #typeTableName#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="flag">
	       			t.FLAG like #flag#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="usetype">
	       			t.USETYPE like #usetype#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isShowType">
	       			t.IS_SHOW_TYPE like #isShowType#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="treeSort">
	       			t.TREE_SORT like #treeSort#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdAbstractType">
    <![CDATA[
        INSERT INTO
        T_MD_ABSTRACT_TYPE (
        	CUID ,
        	PARENT_TYPE_CUID ,
        	BM_CLASSID ,
        	LABEL_CN ,
        	RELATED_RESCLASS_CUID ,
        	LABEL_ATTR_ID ,
        	LABEL_JOIN_CHAR ,
        	OBJECT_SELECT_XTYPE ,
        	IS_SHOW_ADD_BTN ,
        	IS_SHOW_EDIT_BTN ,
        	IS_SHOW_DELETE_BTN ,
        	IS_SHOW_EXPORT_BTN ,
        	IS_SHOW_IMPORT_BTN ,
        	IS_VIRTUAL ,
        	PK_COL ,
        	TYPE_TABLE_NAME ,
        	FLAG ,
        	USETYPE ,
        	IS_SHOW_TYPE ,
        	TREE_SORT 
        ) VALUES (
        	#cuid# ,
        	#parentTypeCuid# ,
        	#bmClassid# ,
        	#labelCn# ,
        	#relatedResclassCuid# ,
        	#labelAttrId# ,
        	#labelJoinChar# ,
        	#objectSelectXtype# ,
        	#isShowAddBtn# ,
        	#isShowEditBtn# ,
        	#isShowDeleteBtn# ,
        	#isShowExportBtn# ,
        	#isShowImportBtn# ,
        	#isVirtual# ,
        	#pkCol# ,
        	#typeTableName# ,
        	#flag# ,
        	#usetype# ,
        	#isShowType# ,
        	#treeSort# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdAbstractType">
    <![CDATA[
        UPDATE T_MD_ABSTRACT_TYPE SET
	        PARENT_TYPE_CUID = #parentTypeCuid# ,
	        BM_CLASSID = #bmClassid# ,
	        LABEL_CN = #labelCn# ,
	        RELATED_RESCLASS_CUID = #relatedResclassCuid# ,
	        LABEL_ATTR_ID = #labelAttrId# ,
	        LABEL_JOIN_CHAR = #labelJoinChar# ,
	        OBJECT_SELECT_XTYPE = #objectSelectXtype# ,
	        IS_SHOW_ADD_BTN = #isShowAddBtn# ,
	        IS_SHOW_EDIT_BTN = #isShowEditBtn# ,
	        IS_SHOW_DELETE_BTN = #isShowDeleteBtn# ,
	        IS_SHOW_EXPORT_BTN = #isShowExportBtn# ,
	        IS_SHOW_IMPORT_BTN = #isShowImportBtn# ,
	        IS_VIRTUAL = #isVirtual# ,
	        PK_COL = #pkCol# ,
	        TYPE_TABLE_NAME = #typeTableName# ,
	        FLAG = #flag# ,
	        USETYPE = #usetype# ,
	        IS_SHOW_TYPE = #isShowType# ,
	        TREE_SORT = #treeSort# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_ABSTRACT_TYPE where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_ABSTRACT_TYPE t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdAbstractTypeResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_ABSTRACT_TYPE t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_ABSTRACT_TYPE t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdAbstractTypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_ABSTRACT_TYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultClass="java.util.HashMap">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_ABSTRACT_TYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdAbstractTypeResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_ABSTRACT_TYPE t
	    ]]>
    </select>
</sqlMap>
