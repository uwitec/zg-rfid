<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdLogicdeviceLogicport">

    <typeAlias alias="tmdLogicdeviceLogicport" type="com.boco.frame.meta.base.model.TmdLogicdeviceLogicport"/>

    <resultMap id="tmdLogicdeviceLogicportResult" class="tmdLogicdeviceLogicport">
        <result property="cuid" column="CUID"/>
        <result property="relatedLdeviceBmclassid" column="RELATED_LDEVICE_BMCLASSID"/>
        <result property="relatedLportBmclassid" column="RELATED_LPORT_BMCLASSID"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.RELATED_LDEVICE_BMCLASSID,
        	t.RELATED_LPORT_BMCLASSID
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedLdeviceBmclassid">
	       			t.RELATED_LDEVICE_BMCLASSID like #relatedLdeviceBmclassid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedLportBmclassid">
	       			t.RELATED_LPORT_BMCLASSID like #relatedLportBmclassid#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdLogicdeviceLogicport">
    <![CDATA[
        INSERT INTO
        T_MD_LOGICDEVICE_LOGICPORT (
        	CUID ,
        	RELATED_LDEVICE_BMCLASSID ,
        	RELATED_LPORT_BMCLASSID 
        ) VALUES (
        	#cuid# ,
        	#relatedLdeviceBmclassid# ,
        	#relatedLportBmclassid# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdLogicdeviceLogicport">
    <![CDATA[
        UPDATE T_MD_LOGICDEVICE_LOGICPORT SET
	        RELATED_LDEVICE_BMCLASSID = #relatedLdeviceBmclassid# ,
	        RELATED_LPORT_BMCLASSID = #relatedLportBmclassid# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_LOGICDEVICE_LOGICPORT where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_LOGICDEVICE_LOGICPORT t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdLogicdeviceLogicportResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_LOGICDEVICE_LOGICPORT t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_LOGICDEVICE_LOGICPORT t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdLogicdeviceLogicportResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_LOGICDEVICE_LOGICPORT t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdLogicdeviceLogicportResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_LOGICDEVICE_LOGICPORT t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdLogicdeviceLogicportResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_LOGICDEVICE_LOGICPORT t
	    ]]>
    </select>
</sqlMap>
