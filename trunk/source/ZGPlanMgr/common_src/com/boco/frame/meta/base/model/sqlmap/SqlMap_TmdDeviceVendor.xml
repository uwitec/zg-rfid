<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdDeviceVendor">

    <typeAlias alias="tmdDeviceVendor" type="com.boco.frame.meta.base.model.TmdDeviceVendor"/>

    <resultMap id="tmdDeviceVendorResult" class="tmdDeviceVendor">
        <result property="labelCn" column="LABEL_CN"/>
        <result property="remark" column="REMARK"/>
        <result property="zipcode" column="ZIPCODE"/>
        <result property="contactor" column="CONTACTOR"/>
        <result property="facisimile" column="FACISIMILE"/>
        <result property="abbreviation" column="ABBREVIATION"/>
        <result property="telephone" column="TELEPHONE"/>
        <result property="homePage" column="HOME_PAGE"/>
        <result property="emailAddress" column="EMAIL_ADDRESS"/>
        <result property="contactAddress" column="CONTACT_ADDRESS"/>
        <result property="imagePath" column="IMAGE_PATH"/>
        <result property="flag" column="FLAG"/>
        <result property="pkCuid" column="PK_CUID"/>
        <result property="cuid" column="CUID"/>
        <result property="colCuid" column="COL_CUID"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.LABEL_CN,
        	t.REMARK,
        	t.ZIPCODE,
        	t.CONTACTOR,
        	t.FACISIMILE,
        	t.ABBREVIATION,
        	t.TELEPHONE,
        	t.HOME_PAGE,
        	t.EMAIL_ADDRESS,
        	t.CONTACT_ADDRESS,
        	t.IMAGE_PATH,
        	t.FLAG,
        	t.PK_CUID,
        	t.CUID,
        	t.COL_CUID
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="labelCn">
	       			t.LABEL_CN like #labelCn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="remark">
	       			t.REMARK like #remark#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="zipcode">
	       			t.ZIPCODE like #zipcode#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="contactor">
	       			t.CONTACTOR like #contactor#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="facisimile">
	       			t.FACISIMILE like #facisimile#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="abbreviation">
	       			t.ABBREVIATION like #abbreviation#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="telephone">
	       			t.TELEPHONE like #telephone#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="homePage">
	       			t.HOME_PAGE like #homePage#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="emailAddress">
	       			t.EMAIL_ADDRESS like #emailAddress#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="contactAddress">
	       			t.CONTACT_ADDRESS like #contactAddress#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="imagePath">
	       			t.IMAGE_PATH like #imagePath#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="flag">
	       			t.FLAG like #flag#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="pkCuid">
	       			t.PK_CUID like #pkCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="colCuid">
	       			t.COL_CUID like #colCuid#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdDeviceVendor">
    <![CDATA[
        INSERT INTO
        T_MD_DEVICE_VENDOR (
        	LABEL_CN ,
        	REMARK ,
        	ZIPCODE ,
        	CONTACTOR ,
        	FACISIMILE ,
        	ABBREVIATION ,
        	TELEPHONE ,
        	HOME_PAGE ,
        	EMAIL_ADDRESS ,
        	CONTACT_ADDRESS ,
        	IMAGE_PATH ,
        	FLAG ,
        	PK_CUID ,
        	CUID ,
        	COL_CUID 
        ) VALUES (
        	#labelCn# ,
        	#remark# ,
        	#zipcode# ,
        	#contactor# ,
        	#facisimile# ,
        	#abbreviation# ,
        	#telephone# ,
        	#homePage# ,
        	#emailAddress# ,
        	#contactAddress# ,
        	#imagePath# ,
        	#flag# ,
        	#pkCuid# ,
        	#cuid# ,
        	#colCuid# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="pkCuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdDeviceVendor">
    <![CDATA[
        UPDATE T_MD_DEVICE_VENDOR SET
	        LABEL_CN = #labelCn# ,
	        REMARK = #remark# ,
	        ZIPCODE = #zipcode# ,
	        CONTACTOR = #contactor# ,
	        FACISIMILE = #facisimile# ,
	        ABBREVIATION = #abbreviation# ,
	        TELEPHONE = #telephone# ,
	        HOME_PAGE = #homePage# ,
	        EMAIL_ADDRESS = #emailAddress# ,
	        CONTACT_ADDRESS = #contactAddress# ,
	        IMAGE_PATH = #imagePath# ,
	        FLAG = #flag# ,
	        CUID = #cuid# ,
	        COL_CUID = #colCuid# 
        WHERE 
	        PK_CUID = #pkCuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_DEVICE_VENDOR where
        PK_CUID = #pkCuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_DEVICE_VENDOR t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdDeviceVendorResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_DEVICE_VENDOR t
	        where 
		        t.PK_CUID = #pkCuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_DEVICE_VENDOR t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdDeviceVendorResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_DEVICE_VENDOR t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdDeviceVendorResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_DEVICE_VENDOR t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdDeviceVendorResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_DEVICE_VENDOR t
	    ]]>
    </select>
</sqlMap>
