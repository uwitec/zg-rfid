<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdRacktypeunit">

    <typeAlias alias="tmdRacktypeunit" type="com.boco.frame.meta.base.model.TmdRacktypeunit"/>

    <resultMap id="tmdRacktypeunitResult" class="tmdRacktypeunit">
        <result property="cuid" column="CUID"/>
        <result property="relatedRacktypeCuid" column="RELATED_RACKTYPE_CUID"/>
        <result property="sequence" column="SEQUENCE"/>
        <result property="rowinrack" column="ROWINRACK"/>
        <result property="colinrack" column="COLINRACK"/>
        <result property="orientation" column="ORIENTATION"/>
        <result property="isavailable" column="ISAVAILABLE"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.RELATED_RACKTYPE_CUID,
        	t.SEQUENCE,
        	t.ROWINRACK,
        	t.COLINRACK,
        	t.ORIENTATION,
        	t.ISAVAILABLE
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedRacktypeCuid">
	       			t.RELATED_RACKTYPE_CUID like #relatedRacktypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="sequence">
	       			t.SEQUENCE = #sequence#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="rowinrack">
	       			t.ROWINRACK = #rowinrack#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="colinrack">
	       			t.COLINRACK = #colinrack#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="orientation">
	       			t.ORIENTATION = #orientation#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isavailable">
	       			t.ISAVAILABLE = #isavailable#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdRacktypeunit">
    <![CDATA[
        INSERT INTO
        T_MD_RACKTYPEUNIT (
        	CUID ,
        	RELATED_RACKTYPE_CUID ,
        	SEQUENCE ,
        	ROWINRACK ,
        	COLINRACK ,
        	ORIENTATION ,
        	ISAVAILABLE 
        ) VALUES (
        	#cuid# ,
        	#relatedRacktypeCuid# ,
        	#sequence# ,
        	#rowinrack# ,
        	#colinrack# ,
        	#orientation# ,
        	#isavailable# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdRacktypeunit">
    <![CDATA[
        UPDATE T_MD_RACKTYPEUNIT SET
	        RELATED_RACKTYPE_CUID = #relatedRacktypeCuid# ,
	        SEQUENCE = #sequence# ,
	        ROWINRACK = #rowinrack# ,
	        COLINRACK = #colinrack# ,
	        ORIENTATION = #orientation# ,
	        ISAVAILABLE = #isavailable# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_RACKTYPEUNIT where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_RACKTYPEUNIT t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdRacktypeunitResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RACKTYPEUNIT t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_RACKTYPEUNIT t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdRacktypeunitResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RACKTYPEUNIT t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdRacktypeunitResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RACKTYPEUNIT t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdRacktypeunitResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_RACKTYPEUNIT t
	    ]]>
    </select>
</sqlMap>
