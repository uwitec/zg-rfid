<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdDevicetype">

    <typeAlias alias="tmdDevicetype" type="com.boco.frame.meta.base.model.TmdDevicetype"/>

    <resultMap id="tmdDevicetypeResult" class="tmdDevicetype">
        <result property="cuid" column="CUID"/>
        <result property="bmClassid" column="BM_CLASSID"/>
        <result property="labelCn" column="LABEL_CN"/>
        <result property="relatedManufacturerCuid" column="RELATED_MANUFACTURER_CUID"/>
        <result property="version" column="VERSION"/>
        <result property="shelfstartpos" column="SHELFSTARTPOS"/>
        <result property="dynshelf" column="DYNSHELF"/>
        <result property="description" column="DESCRIPTION"/>
        <result property="portstartpos" column="PORTSTARTPOS"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.BM_CLASSID,
        	t.LABEL_CN,
        	t.RELATED_MANUFACTURER_CUID,
        	t.VERSION,
        	t.SHELFSTARTPOS,
        	t.DYNSHELF,
        	t.DESCRIPTION,
        	t.PORTSTARTPOS
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="bmClassid">
	       			t.BM_CLASSID like #bmClassid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="labelCn">
	       			t.LABEL_CN like #labelCn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedManufacturerCuid">
	       			t.RELATED_MANUFACTURER_CUID like #relatedManufacturerCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="version">
	       			t.VERSION like #version#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="shelfstartpos">
	       			t.SHELFSTARTPOS = #shelfstartpos#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="dynshelf">
	       			t.DYNSHELF = #dynshelf#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="description">
	       			t.DESCRIPTION like #description#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="portstartpos">
	       			t.PORTSTARTPOS = #portstartpos#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdDevicetype">
    <![CDATA[
        INSERT INTO
        T_MD_DEVICETYPE (
        	CUID ,
        	BM_CLASSID ,
        	LABEL_CN ,
        	RELATED_MANUFACTURER_CUID ,
        	VERSION ,
        	SHELFSTARTPOS ,
        	DYNSHELF ,
        	DESCRIPTION ,
        	PORTSTARTPOS 
        ) VALUES (
        	#cuid# ,
        	#bmClassid# ,
        	#labelCn# ,
        	#relatedManufacturerCuid# ,
        	#version# ,
        	#shelfstartpos# ,
        	#dynshelf# ,
        	#description# ,
        	#portstartpos# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdDevicetype">
    <![CDATA[
        UPDATE T_MD_DEVICETYPE SET
	        BM_CLASSID = #bmClassid# ,
	        LABEL_CN = #labelCn# ,
	        RELATED_MANUFACTURER_CUID = #relatedManufacturerCuid# ,
	        VERSION = #version# ,
	        SHELFSTARTPOS = #shelfstartpos# ,
	        DYNSHELF = #dynshelf# ,
	        DESCRIPTION = #description# ,
	        PORTSTARTPOS = #portstartpos# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_DEVICETYPE where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_DEVICETYPE t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdDevicetypeResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_DEVICETYPE t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_DEVICETYPE t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdDevicetypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_DEVICETYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdDevicetypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_DEVICETYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdDevicetypeResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_DEVICETYPE t
	    ]]>
    </select>
</sqlMap>
