<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdRestypeRestype">

    <typeAlias alias="tmdRestypeRestype" type="com.boco.frame.meta.base.model.TmdRestypeRestype"/>

    <resultMap id="tmdRestypeRestypeResult" class="tmdRestypeRestype">
        <result property="cuid" column="CUID"/>
        <result property="labelCn" column="LABEL_CN"/>
        <result property="relatedLeftType" column="RELATED_LEFT_TYPE"/>
        <result property="leftInhibit" column="LEFT_INHIBIT"/>
        <result property="relatedRightType" column="RELATED_RIGHT_TYPE"/>
        <result property="rightInhibit" column="RIGHT_INHIBIT"/>
        <result property="leftAttrName" column="LEFT_ATTR_NAME"/>
        <result property="rightAttrName" column="RIGHT_ATTR_NAME"/>
        <result property="centerTableName" column="CENTER_TABLE_NAME"/>
        <result property="description" column="DESCRIPTION"/>
        <result property="dicCode" column="DIC_CODE"/>
        <result property="isHaveInstance" column="IS_HAVE_INSTANCE"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.LABEL_CN,
        	t.RELATED_LEFT_TYPE,
        	t.LEFT_INHIBIT,
        	t.RELATED_RIGHT_TYPE,
        	t.RIGHT_INHIBIT,
        	t.LEFT_ATTR_NAME,
        	t.RIGHT_ATTR_NAME,
        	t.CENTER_TABLE_NAME,
        	t.DESCRIPTION,
        	t.DIC_CODE,
        	t.IS_HAVE_INSTANCE
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="labelCn">
	       			t.LABEL_CN like #labelCn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedLeftType">
	       			t.RELATED_LEFT_TYPE like #relatedLeftType#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="leftInhibit">
	       			t.LEFT_INHIBIT like #leftInhibit#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedRightType">
	       			t.RELATED_RIGHT_TYPE like #relatedRightType#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="rightInhibit">
	       			t.RIGHT_INHIBIT like #rightInhibit#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="leftAttrName">
	       			t.LEFT_ATTR_NAME like #leftAttrName#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="rightAttrName">
	       			t.RIGHT_ATTR_NAME like #rightAttrName#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="centerTableName">
	       			t.CENTER_TABLE_NAME like #centerTableName#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="description">
	       			t.DESCRIPTION like #description#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="dicCode">
	       			t.DIC_CODE like #dicCode#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isHaveInstance">
	       			t.IS_HAVE_INSTANCE = #isHaveInstance#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdRestypeRestype">
    <![CDATA[
        INSERT INTO
        T_MD_RESTYPE_RESTYPE (
        	CUID ,
        	LABEL_CN ,
        	RELATED_LEFT_TYPE ,
        	LEFT_INHIBIT ,
        	RELATED_RIGHT_TYPE ,
        	RIGHT_INHIBIT ,
        	LEFT_ATTR_NAME ,
        	RIGHT_ATTR_NAME ,
        	CENTER_TABLE_NAME ,
        	DESCRIPTION ,
        	DIC_CODE ,
        	IS_HAVE_INSTANCE 
        ) VALUES (
        	#cuid# ,
        	#labelCn# ,
        	#relatedLeftType# ,
        	#leftInhibit# ,
        	#relatedRightType# ,
        	#rightInhibit# ,
        	#leftAttrName# ,
        	#rightAttrName# ,
        	#centerTableName# ,
        	#description# ,
        	#dicCode# ,
        	#isHaveInstance# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdRestypeRestype">
    <![CDATA[
        UPDATE T_MD_RESTYPE_RESTYPE SET
	        LABEL_CN = #labelCn# ,
	        RELATED_LEFT_TYPE = #relatedLeftType# ,
	        LEFT_INHIBIT = #leftInhibit# ,
	        RELATED_RIGHT_TYPE = #relatedRightType# ,
	        RIGHT_INHIBIT = #rightInhibit# ,
	        LEFT_ATTR_NAME = #leftAttrName# ,
	        RIGHT_ATTR_NAME = #rightAttrName# ,
	        CENTER_TABLE_NAME = #centerTableName# ,
	        DESCRIPTION = #description# ,
	        DIC_CODE = #dicCode# ,
	        IS_HAVE_INSTANCE = #isHaveInstance# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_RESTYPE_RESTYPE where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_RESTYPE_RESTYPE t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdRestypeRestypeResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RESTYPE_RESTYPE t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_RESTYPE_RESTYPE t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdRestypeRestypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RESTYPE_RESTYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdRestypeRestypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RESTYPE_RESTYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdRestypeRestypeResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_RESTYPE_RESTYPE t
	    ]]>
    </select>
</sqlMap>
