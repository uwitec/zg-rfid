<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdNetworktypeNodetype">

    <typeAlias alias="tmdNetworktypeNodetype" type="com.boco.frame.meta.base.model.TmdNetworktypeNodetype"/>

    <resultMap id="tmdNetworktypeNodetypeResult" class="tmdNetworktypeNodetype">
        <result property="cuid" column="CUID"/>
        <result property="relatedNetworktypeCuid" column="RELATED_NETWORKTYPE_CUID"/>
        <result property="relatedNodetypeCuid" column="RELATED_NODETYPE_CUID"/>
        <result property="nodefilter" column="NODEFILTER"/>
        <result property="remark" column="REMARK"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.RELATED_NETWORKTYPE_CUID,
        	t.RELATED_NODETYPE_CUID,
        	t.NODEFILTER,
        	t.REMARK
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedNetworktypeCuid">
	       			t.RELATED_NETWORKTYPE_CUID like #relatedNetworktypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedNodetypeCuid">
	       			t.RELATED_NODETYPE_CUID like #relatedNodetypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="nodefilter">
	       			t.NODEFILTER like #nodefilter#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="remark">
	       			t.REMARK like #remark#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdNetworktypeNodetype">
    <![CDATA[
        INSERT INTO
        T_MD_NETWORKTYPE_NODETYPE (
        	CUID ,
        	RELATED_NETWORKTYPE_CUID ,
        	RELATED_NODETYPE_CUID ,
        	NODEFILTER ,
        	REMARK 
        ) VALUES (
        	#cuid# ,
        	#relatedNetworktypeCuid# ,
        	#relatedNodetypeCuid# ,
        	#nodefilter# ,
        	#remark# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdNetworktypeNodetype">
    <![CDATA[
        UPDATE T_MD_NETWORKTYPE_NODETYPE SET
	        RELATED_NETWORKTYPE_CUID = #relatedNetworktypeCuid# ,
	        RELATED_NODETYPE_CUID = #relatedNodetypeCuid# ,
	        NODEFILTER = #nodefilter# ,
	        REMARK = #remark# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_NETWORKTYPE_NODETYPE where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_NETWORKTYPE_NODETYPE t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdNetworktypeNodetypeResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_NETWORKTYPE_NODETYPE t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_NETWORKTYPE_NODETYPE t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdNetworktypeNodetypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_NETWORKTYPE_NODETYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdNetworktypeNodetypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_NETWORKTYPE_NODETYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdNetworktypeNodetypeResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_NETWORKTYPE_NODETYPE t
	    ]]>
    </select>
</sqlMap>
