<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdAttribute">

    <typeAlias alias="tmdAttribute" type="com.boco.frame.meta.base.model.TmdAttribute"/>

    <resultMap id="tmdAttributeResult" class="tmdAttribute">
        <result property="cuid" column="CUID"/>
        <result property="labelCn" column="LABEL_CN"/>
        <result property="relatedAttrsetCuid" column="RELATED_ATTRSET_CUID"/>
        <result property="columnName" column="COLUMN_NAME"/>
        <result property="dataType" column="DATA_TYPE"/>
        <result property="sequence" column="SEQUENCE"/>
        <result property="relatedEnumetypeCuid" column="RELATED_ENUMETYPE_CUID"/>
        <result property="isNotnull" column="IS_NOTNULL"/>
        <result property="isReadable" column="IS_READABLE"/>
        <result property="isSystem" column="IS_SYSTEM"/>
        <result property="defaultValue" column="DEFAULT_VALUE"/>
        <result property="dataSource" column="DATA_SOURCE"/>
        <result property="strAttrMaxlen" column="STR_ATTR_MAXLEN"/>
        <result property="xtype" column="XTYPE"/>
        <result property="verifyExpression" column="VERIFY_EXPRESSION"/>
        <result property="calculateExpression" column="CALCULATE_EXPRESSION"/>
        <result property="description" column="DESCRIPTION"/>
        <result property="isBatchModify" column="IS_BATCH_MODIFY"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.LABEL_CN,
        	t.RELATED_ATTRSET_CUID,
        	t.COLUMN_NAME,
        	t.DATA_TYPE,
        	t.SEQUENCE,
        	t.RELATED_ENUMETYPE_CUID,
        	t.IS_NOTNULL,
        	t.IS_READABLE,
        	t.IS_SYSTEM,
        	t.DEFAULT_VALUE,
        	t.DATA_SOURCE,
        	t.STR_ATTR_MAXLEN,
        	t.XTYPE,
        	t.VERIFY_EXPRESSION,
        	t.CALCULATE_EXPRESSION,
        	t.DESCRIPTION,
        	t.IS_BATCH_MODIFY
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="labelCn">
	       			t.LABEL_CN like #labelCn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedAttrsetCuid">
	       			t.RELATED_ATTRSET_CUID like #relatedAttrsetCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="columnName">
	       			t.COLUMN_NAME like #columnName#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="dataType">
	       			t.DATA_TYPE like #dataType#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="sequence">
	       			t.SEQUENCE = #sequence#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedEnumetypeCuid">
	       			t.RELATED_ENUMETYPE_CUID like #relatedEnumetypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isNotnull">
	       			t.IS_NOTNULL = #isNotnull#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isReadable">
	       			t.IS_READABLE = #isReadable#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isSystem">
	       			t.IS_SYSTEM = #isSystem#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="defaultValue">
	       			t.DEFAULT_VALUE like #defaultValue#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="dataSource">
	       			t.DATA_SOURCE = #dataSource#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="strAttrMaxlen">
	       			t.STR_ATTR_MAXLEN = #strAttrMaxlen#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="xtype">
	       			t.XTYPE like #xtype#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="verifyExpression">
	       			t.VERIFY_EXPRESSION like #verifyExpression#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="calculateExpression">
	       			t.CALCULATE_EXPRESSION like #calculateExpression#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="description">
	       			t.DESCRIPTION like #description#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isBatchModify">
	       			t.IS_BATCH_MODIFY = #isBatchModify#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdAttribute">
    <![CDATA[
        INSERT INTO
        T_MD_ATTRIBUTE (
        	CUID ,
        	LABEL_CN ,
        	RELATED_ATTRSET_CUID ,
        	COLUMN_NAME ,
        	DATA_TYPE ,
        	SEQUENCE ,
        	RELATED_ENUMETYPE_CUID ,
        	IS_NOTNULL ,
        	IS_READABLE ,
        	IS_SYSTEM ,
        	DEFAULT_VALUE ,
        	DATA_SOURCE ,
        	STR_ATTR_MAXLEN ,
        	XTYPE ,
        	VERIFY_EXPRESSION ,
        	CALCULATE_EXPRESSION ,
        	DESCRIPTION ,
        	IS_BATCH_MODIFY 
        ) VALUES (
        	#cuid# ,
        	#labelCn# ,
        	#relatedAttrsetCuid# ,
        	#columnName# ,
        	#dataType# ,
        	#sequence# ,
        	#relatedEnumetypeCuid# ,
        	#isNotnull# ,
        	#isReadable# ,
        	#isSystem# ,
        	#defaultValue# ,
        	#dataSource# ,
        	#strAttrMaxlen# ,
        	#xtype# ,
        	#verifyExpression# ,
        	#calculateExpression# ,
        	#description# ,
        	#isBatchModify# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdAttribute">
    <![CDATA[
        UPDATE T_MD_ATTRIBUTE SET
	        LABEL_CN = #labelCn# ,
	        RELATED_ATTRSET_CUID = #relatedAttrsetCuid# ,
	        COLUMN_NAME = #columnName# ,
	        DATA_TYPE = #dataType# ,
	        SEQUENCE = #sequence# ,
	        RELATED_ENUMETYPE_CUID = #relatedEnumetypeCuid# ,
	        IS_NOTNULL = #isNotnull# ,
	        IS_READABLE = #isReadable# ,
	        IS_SYSTEM = #isSystem# ,
	        DEFAULT_VALUE = #defaultValue# ,
	        DATA_SOURCE = #dataSource# ,
	        STR_ATTR_MAXLEN = #strAttrMaxlen# ,
	        XTYPE = #xtype# ,
	        VERIFY_EXPRESSION = #verifyExpression# ,
	        CALCULATE_EXPRESSION = #calculateExpression# ,
	        DESCRIPTION = #description# ,
	        IS_BATCH_MODIFY = #isBatchModify# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_ATTRIBUTE where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_ATTRIBUTE t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdAttributeResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_ATTRIBUTE t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_ATTRIBUTE t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdAttributeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_ATTRIBUTE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdAttributeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_ATTRIBUTE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdAttributeResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_ATTRIBUTE t
	    ]]>
    </select>
</sqlMap>
