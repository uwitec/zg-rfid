<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdAttrset">

    <typeAlias alias="tmdAttrset" type="com.boco.frame.meta.base.model.TmdAttrset"/>

    <resultMap id="tmdAttrsetResult" class="tmdAttrset">
        <result property="cuid" column="CUID"/>
        <result property="labelCn" column="LABEL_CN"/>
        <result property="tableName" column="TABLE_NAME"/>
        <result property="description" column="DESCRIPTION"/>
        <result property="pkCol" column="PK_COL"/>
        <result property="relatedResTypeCuid" column="RELATED_RES_TYPE_CUID"/>
        <result property="isBasic" column="IS_BASIC"/>
        <result property="isSystem" column="IS_SYSTEM"/>
        <result property="isStatic" column="IS_STATIC"/>
        <result property="isResource" column="IS_RESOURCE"/>
        <result property="usetype" column="USETYPE"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.LABEL_CN,
        	t.TABLE_NAME,
        	t.DESCRIPTION,
        	t.PK_COL,
        	t.RELATED_RES_TYPE_CUID,
        	t.IS_BASIC,
        	t.IS_SYSTEM,
        	t.IS_STATIC,
        	t.IS_RESOURCE,
        	t.USETYPE
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="labelCn">
	       			t.LABEL_CN like #labelCn#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="tableName">
	       			t.TABLE_NAME like #tableName#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="description">
	       			t.DESCRIPTION like #description#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="pkCol">
	       			t.PK_COL like #pkCol#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedResTypeCuid">
	       			t.RELATED_RES_TYPE_CUID like #relatedResTypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isBasic">
	       			t.IS_BASIC = #isBasic#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isSystem">
	       			t.IS_SYSTEM = #isSystem#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isStatic">
	       			t.IS_STATIC = #isStatic#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="isResource">
	       			t.IS_RESOURCE = #isResource#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="usetype">
	       			t.USETYPE like #usetype#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdAttrset">
    <![CDATA[
        INSERT INTO
        T_MD_ATTRSET (
        	CUID ,
        	LABEL_CN ,
        	TABLE_NAME ,
        	DESCRIPTION ,
        	PK_COL ,
        	RELATED_RES_TYPE_CUID ,
        	IS_BASIC ,
        	IS_SYSTEM ,
        	IS_STATIC ,
        	IS_RESOURCE ,
        	USETYPE 
        ) VALUES (
        	#cuid# ,
        	#labelCn# ,
        	#tableName# ,
        	#description# ,
        	#pkCol# ,
        	#relatedResTypeCuid# ,
        	#isBasic# ,
        	#isSystem# ,
        	#isStatic# ,
        	#isResource# ,
        	#usetype# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdAttrset">
    <![CDATA[
        UPDATE T_MD_ATTRSET SET
	        LABEL_CN = #labelCn# ,
	        TABLE_NAME = #tableName# ,
	        DESCRIPTION = #description# ,
	        PK_COL = #pkCol# ,
	        RELATED_RES_TYPE_CUID = #relatedResTypeCuid# ,
	        IS_BASIC = #isBasic# ,
	        IS_SYSTEM = #isSystem# ,
	        IS_STATIC = #isStatic# ,
	        IS_RESOURCE = #isResource# ,
	        USETYPE = #usetype# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_ATTRSET where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_ATTRSET t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdAttrsetResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_ATTRSET t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_ATTRSET t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdAttrsetResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_ATTRSET t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdAttrsetResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_ATTRSET t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdAttrsetResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_ATTRSET t
	    ]]>
    </select>
</sqlMap>
