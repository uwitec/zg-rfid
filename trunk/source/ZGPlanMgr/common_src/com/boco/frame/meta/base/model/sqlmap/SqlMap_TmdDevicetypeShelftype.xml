<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdDevicetypeShelftype">

    <typeAlias alias="tmdDevicetypeShelftype" type="com.boco.frame.meta.base.model.TmdDevicetypeShelftype"/>

    <resultMap id="tmdDevicetypeShelftypeResult" class="tmdDevicetypeShelftype">
        <result property="cuid" column="CUID"/>
        <result property="relatedDevicetypeCuid" column="RELATED_DEVICETYPE_CUID"/>
        <result property="relatedShelftypeCuid" column="RELATED_SHELFTYPE_CUID"/>
        <result property="sequence" column="SEQUENCE"/>
        <result property="rowindevice" column="ROWINDEVICE"/>
        <result property="colindevice" column="COLINDEVICE"/>
        <result property="orientation" column="ORIENTATION"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.RELATED_DEVICETYPE_CUID,
        	t.RELATED_SHELFTYPE_CUID,
        	t.SEQUENCE,
        	t.ROWINDEVICE,
        	t.COLINDEVICE,
        	t.ORIENTATION
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedDevicetypeCuid">
	       			t.RELATED_DEVICETYPE_CUID like #relatedDevicetypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedShelftypeCuid">
	       			t.RELATED_SHELFTYPE_CUID like #relatedShelftypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="sequence">
	       			t.SEQUENCE = #sequence#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="rowindevice">
	       			t.ROWINDEVICE = #rowindevice#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="colindevice">
	       			t.COLINDEVICE = #colindevice#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="orientation">
	       			t.ORIENTATION = #orientation#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdDevicetypeShelftype">
    <![CDATA[
        INSERT INTO
        T_MD_DEVICETYPE_SHELFTYPE (
        	CUID ,
        	RELATED_DEVICETYPE_CUID ,
        	RELATED_SHELFTYPE_CUID ,
        	SEQUENCE ,
        	ROWINDEVICE ,
        	COLINDEVICE ,
        	ORIENTATION 
        ) VALUES (
        	#cuid# ,
        	#relatedDevicetypeCuid# ,
        	#relatedShelftypeCuid# ,
        	#sequence# ,
        	#rowindevice# ,
        	#colindevice# ,
        	#orientation# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdDevicetypeShelftype">
    <![CDATA[
        UPDATE T_MD_DEVICETYPE_SHELFTYPE SET
	        RELATED_DEVICETYPE_CUID = #relatedDevicetypeCuid# ,
	        RELATED_SHELFTYPE_CUID = #relatedShelftypeCuid# ,
	        SEQUENCE = #sequence# ,
	        ROWINDEVICE = #rowindevice# ,
	        COLINDEVICE = #colindevice# ,
	        ORIENTATION = #orientation# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_DEVICETYPE_SHELFTYPE where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_DEVICETYPE_SHELFTYPE t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdDevicetypeShelftypeResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_DEVICETYPE_SHELFTYPE t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_DEVICETYPE_SHELFTYPE t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdDevicetypeShelftypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_DEVICETYPE_SHELFTYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdDevicetypeShelftypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_DEVICETYPE_SHELFTYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdDevicetypeShelftypeResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_DEVICETYPE_SHELFTYPE t
	    ]]>
    </select>
</sqlMap>
