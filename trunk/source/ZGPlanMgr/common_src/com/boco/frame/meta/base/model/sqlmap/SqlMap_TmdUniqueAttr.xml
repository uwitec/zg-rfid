<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdUniqueAttr">

    <typeAlias alias="tmdUniqueAttr" type="com.boco.frame.meta.base.model.TmdUniqueAttr"/>

    <resultMap id="tmdUniqueAttrResult" class="tmdUniqueAttr">
        <result property="cuid" column="CUID"/>
        <result property="relatedUniqueCuid" column="RELATED_UNIQUE_CUID"/>
        <result property="relatedAttrsetCuid" column="RELATED_ATTRSET_CUID"/>
        <result property="relatedAttrCuid" column="RELATED_ATTR_CUID"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.RELATED_UNIQUE_CUID,
        	t.RELATED_ATTRSET_CUID,
        	t.RELATED_ATTR_CUID
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedUniqueCuid">
	       			t.RELATED_UNIQUE_CUID like #relatedUniqueCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedAttrsetCuid">
	       			t.RELATED_ATTRSET_CUID like #relatedAttrsetCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedAttrCuid">
	       			t.RELATED_ATTR_CUID like #relatedAttrCuid#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdUniqueAttr">
    <![CDATA[
        INSERT INTO
        T_MD_UNIQUE_ATTR (
        	CUID ,
        	RELATED_UNIQUE_CUID ,
        	RELATED_ATTRSET_CUID ,
        	RELATED_ATTR_CUID 
        ) VALUES (
        	#cuid# ,
        	#relatedUniqueCuid# ,
        	#relatedAttrsetCuid# ,
        	#relatedAttrCuid# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdUniqueAttr">
    <![CDATA[
        UPDATE T_MD_UNIQUE_ATTR SET
	        RELATED_UNIQUE_CUID = #relatedUniqueCuid# ,
	        RELATED_ATTRSET_CUID = #relatedAttrsetCuid# ,
	        RELATED_ATTR_CUID = #relatedAttrCuid# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_UNIQUE_ATTR where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_UNIQUE_ATTR t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdUniqueAttrResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_UNIQUE_ATTR t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_UNIQUE_ATTR t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdUniqueAttrResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_UNIQUE_ATTR t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdUniqueAttrResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_UNIQUE_ATTR t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdUniqueAttrResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_UNIQUE_ATTR t
	    ]]>
    </select>
</sqlMap>
