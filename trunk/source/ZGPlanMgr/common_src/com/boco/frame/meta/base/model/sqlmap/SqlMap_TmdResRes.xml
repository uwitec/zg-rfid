<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdResRes">

    <typeAlias alias="tmdResRes" type="com.boco.frame.meta.base.model.TmdResRes"/>

    <resultMap id="tmdResResResult" class="tmdResRes">
        <result property="cuid" column="CUID"/>
        <result property="relatedLeftCuid" column="RELATED_LEFT_CUID"/>
        <result property="relatedLtypeCuid" column="RELATED_L_TYPE_CUID"/>
        <result property="relatedRightCuid" column="RELATED_RIGHT_CUID"/>
        <result property="relatedRtypeCuid" column="RELATED_R_TYPE_CUID"/>
        <result property="relatedRelationCuid" column="RELATED_RELATION_CUID"/>
        <result property="desc1" column="DESC1"/>
        <result property="desc2" column="DESC2"/>
        <result property="desc3" column="DESC3"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.RELATED_LEFT_CUID,
        	t.RELATED_L_TYPE_CUID,
        	t.RELATED_RIGHT_CUID,
        	t.RELATED_R_TYPE_CUID,
        	t.RELATED_RELATION_CUID,
        	t.DESC1,
        	t.DESC2,
        	t.DESC3
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedLeftCuid">
	       			t.RELATED_LEFT_CUID like #relatedLeftCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedLtypeCuid">
	       			t.RELATED_L_TYPE_CUID like #relatedLtypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedRightCuid">
	       			t.RELATED_RIGHT_CUID like #relatedRightCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedRtypeCuid">
	       			t.RELATED_R_TYPE_CUID like #relatedRtypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedRelationCuid">
	       			t.RELATED_RELATION_CUID like #relatedRelationCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="desc1">
	       			t.DESC1 like #desc1#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="desc2">
	       			t.DESC2 like #desc2#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="desc3">
	       			t.DESC3 like #desc3#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdResRes">
    <![CDATA[
        INSERT INTO
        T_MD_RES_RES (
        	CUID ,
        	RELATED_LEFT_CUID ,
        	RELATED_L_TYPE_CUID ,
        	RELATED_RIGHT_CUID ,
        	RELATED_R_TYPE_CUID ,
        	RELATED_RELATION_CUID ,
        	DESC1 ,
        	DESC2 ,
        	DESC3 
        ) VALUES (
        	#cuid# ,
        	#relatedLeftCuid# ,
        	#relatedLtypeCuid# ,
        	#relatedRightCuid# ,
        	#relatedRtypeCuid# ,
        	#relatedRelationCuid# ,
        	#desc1# ,
        	#desc2# ,
        	#desc3# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdResRes">
    <![CDATA[
        UPDATE T_MD_RES_RES SET
	        RELATED_LEFT_CUID = #relatedLeftCuid# ,
	        RELATED_L_TYPE_CUID = #relatedLtypeCuid# ,
	        RELATED_RIGHT_CUID = #relatedRightCuid# ,
	        RELATED_R_TYPE_CUID = #relatedRtypeCuid# ,
	        RELATED_RELATION_CUID = #relatedRelationCuid# ,
	        DESC1 = #desc1# ,
	        DESC2 = #desc2# ,
	        DESC3 = #desc3# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_RES_RES where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_RES_RES t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdResResResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RES_RES t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_RES_RES t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdResResResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RES_RES t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdResResResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_RES_RES t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdResResResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_RES_RES t
	    ]]>
    </select>
</sqlMap>
