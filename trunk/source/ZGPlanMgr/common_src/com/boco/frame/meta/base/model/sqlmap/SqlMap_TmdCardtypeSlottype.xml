<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="TmdCardtypeSlottype">

    <typeAlias alias="tmdCardtypeSlottype" type="com.boco.frame.meta.base.model.TmdCardtypeSlottype"/>

    <resultMap id="tmdCardtypeSlottypeResult" class="tmdCardtypeSlottype">
        <result property="cuid" column="CUID"/>
        <result property="relatedCardtypeCuid" column="RELATED_CARDTYPE_CUID"/>
        <result property="relatedSlottypeCuid" column="RELATED_SLOTTYPE_CUID"/>
        <result property="sequence" column="SEQUENCE"/>
        <result property="rowincard" column="ROWINCARD"/>
        <result property="colincard" column="COLINCARD"/>
        <result property="orientation" column="ORIENTATION"/>
    </resultMap>

	<!-- 用于select查询公用抽取的列 -->
	<sql id="commonColumns">
	    <![CDATA[
        	t.CUID,
        	t.RELATED_CARDTYPE_CUID,
        	t.RELATED_SLOTTYPE_CUID,
        	t.SEQUENCE,
        	t.ROWINCARD,
        	t.COLINCARD,
        	t.ORIENTATION
	    ]]>
	</sql>
	
	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	    	<isNotEmpty prepend="AND" property="sqlQueryString">
		    		$sqlQueryString$ 
		    </isNotEmpty>
       		<isNotEmpty prepend="AND" property="cuid">
	       			t.CUID like #cuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedCardtypeCuid">
	       			t.RELATED_CARDTYPE_CUID like #relatedCardtypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="relatedSlottypeCuid">
	       			t.RELATED_SLOTTYPE_CUID like #relatedSlottypeCuid#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="sequence">
	       			t.SEQUENCE = #sequence#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="rowincard">
	       			t.ROWINCARD = #rowincard#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="colincard">
	       			t.COLINCARD = #colincard#
       		</isNotEmpty>
       		<isNotEmpty prepend="AND" property="orientation">
	       			t.ORIENTATION = #orientation#
       		</isNotEmpty>
	    </dynamic>	
	</sql>
	
    <insert id="insert" parameterClass="tmdCardtypeSlottype">
    <![CDATA[
        INSERT INTO
        T_MD_CARDTYPE_SLOTTYPE (
        	CUID ,
        	RELATED_CARDTYPE_CUID ,
        	RELATED_SLOTTYPE_CUID ,
        	SEQUENCE ,
        	ROWINCARD ,
        	COLINCARD ,
        	ORIENTATION 
        ) VALUES (
        	#cuid# ,
        	#relatedCardtypeCuid# ,
        	#relatedSlottypeCuid# ,
        	#sequence# ,
        	#rowincard# ,
        	#colincard# ,
        	#orientation# 
        )
    ]]>
    <!--
    mysql:		type="post" SELECT  select last_insert_id()
    sqlserver:	type="post" SELECT  @@identity  AS  ID
    oracle:		type="pre" 	SELECT  sequenceName.nextval AS ID FROM DUAL
    DB2:		type="pre"	values nextval for sequenceName
    -->
		<selectKey resultClass="java.lang.String" type="pre" keyProperty="cuid" >
            select sys_guid() as id FROM DUAL
        </selectKey>
    </insert>

    <update id="update" parameterClass="tmdCardtypeSlottype">
    <![CDATA[
        UPDATE T_MD_CARDTYPE_SLOTTYPE SET
	        RELATED_CARDTYPE_CUID = #relatedCardtypeCuid# ,
	        RELATED_SLOTTYPE_CUID = #relatedSlottypeCuid# ,
	        SEQUENCE = #sequence# ,
	        ROWINCARD = #rowincard# ,
	        COLINCARD = #colincard# ,
	        ORIENTATION = #orientation# 
        WHERE 
	        CUID = #cuid# 
    ]]>
    </update>
    <delete id="delete" parameterClass="java.lang.String">
    <![CDATA[
        delete from T_MD_CARDTYPE_SLOTTYPE where
        CUID = #cuid# 
    ]]>
    </delete>
    
    <delete id="deleteByProperty">
    <![CDATA[
        delete from T_MD_CARDTYPE_SLOTTYPE t
    ]]>
    	<include refid="dynamicWhere"/>
    </delete>
    
    <select id="getById" resultMap="tmdCardtypeSlottypeResult">
	    select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_CARDTYPE_SLOTTYPE t
	        where 
		        t.CUID = #cuid# 
	    ]]>
    </select>
	 
    <select id="count" resultClass="long">
    <![CDATA[
        select count(*) from T_MD_CARDTYPE_SLOTTYPE t
    ]]>
		<include refid="dynamicWhere"/>    
    </select>
    
    <!--
    	分页查询已经使用Dialect进行分页,也可以不使用Dialect直接编写分页
    	因为分页查询将传 #offset#,#pageSize#,#lastRows# 三个参数,不同的数据库可以根于此三个参数属性应用不同的分页实现
    -->
    <select id="pageSelect" resultMap="tmdCardtypeSlottypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_CARDTYPE_SLOTTYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="findByProperty" resultMap="tmdCardtypeSlottypeResult">
    	select <include refid="commonColumns"/>
	    <![CDATA[
	        from T_MD_CARDTYPE_SLOTTYPE t
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>

	
	<select id="findAll" resultMap="tmdCardtypeSlottypeResult">
		select <include refid="commonColumns"/> 
	    <![CDATA[
	        from T_MD_CARDTYPE_SLOTTYPE t
	    ]]>
    </select>
</sqlMap>
